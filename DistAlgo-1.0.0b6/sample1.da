import sys

class Kavi(process):
    def setup() : pass
    
    def main():
	await(received(('Hello',)))

    def receive(msg=('Hi',), from_=p):
	output("Received from Mani")
	send(('Hello',), to=p)

class Mani(process):
    def setup(k) : pass
    
    def main():
	send(('Hi'), to=k)
	await(received(('Hello',)))
    
    def receive(msg=('Hello',)):
	output("Received from Kavi")

def main():
    k = new(Kavi, num=1)
    m = new(Mani, num=1)
    
    setup(m, k)
    start(k)
    start(m)



